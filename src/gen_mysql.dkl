#include "dkli.dkh"
#!
program
{
    #include "ddm.mysql.dkl"
    
    ref sentences=schema2mysql(@@(@context,"&schema"))
    ref buffer=text.create()
    
    for i=0;i<@count(sentences)
    {
        do text.say(buffer,list.str(sentences,i))
    }

    @context<"output">:"text"
    @context<"text">:text.get(buffer)
}